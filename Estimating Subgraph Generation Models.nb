           RowBox[{"myPrevious", "=", 
            RowBox[{"Last", "[", "myList", "]"}]}], ",", 
           RowBox[{"myProposal", "=", 
            RowBox[{
             RowBox[{"Last", "[", "myList", "]"}], "+", 
