Cell[BoxData[
 RowBox[{
  RowBox[{"generateSubgraphs", "[", "l_", "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"l", "<", "5"}], ",", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"allGraphs", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"Module", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"matrix", "=", 
              RowBox[{"Table", "[", 
               RowBox[{"0", ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", "1", ",", "l"}], "}"}], ",", 
                RowBox[{"{", 
                 RowBox[{"j", ",", "1", ",", "l"}], "}"}]}], "]"}]}], "}"}], 
            ",", 
            RowBox[{
             RowBox[{"Do", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"matrix", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
                RowBox[{"list", "[", 
                 RowBox[{"[", 
                  RowBox[{
                   RowBox[{"l", " ", "i"}], "-", 
                   RowBox[{"Binomial", "[", 
                    RowBox[{"i", ",", "2"}], "]"}], "-", "j", "+", "1"}], 
                  "]"}], "]"}]}], ",", 
               RowBox[{"{", 
                RowBox[{"i", ",", "1", ",", 
                 RowBox[{"l", "-", "1"}]}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"j", ",", 
                 RowBox[{"i", "+", "1"}], ",", "l"}], "}"}]}], "]"}], ";", 
             RowBox[{"matrix", "=", 
              RowBox[{"matrix", "+", 
               RowBox[{"Transpose", "[", "matrix", "]"}]}]}], ";", 
             "matrix"}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"list", ",", 
            RowBox[{"Tuples", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"0", ",", "1"}], "}"}], ",", 
              RowBox[{"Binomial", "[", 
               RowBox[{"l", ",", "2"}], "]"}]}], "]"}]}], "}"}]}], "]"}]}], 
       "}"}], ",", 
      RowBox[{
       RowBox[{"GatherBy", "[", 
        RowBox[{"allGraphs", ",", 
         RowBox[{
          RowBox[{"subgraphIdentifier", "[", "#", "]"}], "&"}]}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{"{", "0", "}"}], "}"}], "}"}]}], "]"}]}]], "Input",
