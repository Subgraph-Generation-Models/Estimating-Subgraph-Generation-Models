            RowBox[{"Pick", "[", 
             RowBox[{"colorList", ",", 
              RowBox[{"Map", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"MemberQ", "[", 
                  RowBox[{"#", ",", "uniqueEdge"}], "]"}], "&"}], ",", 
                "edgeLists"}], "]"}]}], "]"}]}], "]"}], ",", "uniqueEdges"}], 
         "]"}]}], "}"}], ",", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"edgeIndex", "=", 
          RowBox[{"AssociationThread", "[", 
           RowBox[{"uniqueEdges", "\[Rule]", 
            RowBox[{"ConstantArray", "[", 
             RowBox[{"1", ",", 
              RowBox[{"Length", "[", "uniqueEdges", "]"}]}], "]"}]}], "]"}]}],
          "}"}], ",", 
        RowBox[{"Fold", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"SetProperty", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}], ",", 
              RowBox[{"EdgeShapeFunction", "\[Rule]", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"{", 
