Cell[BoxData[
 RowBox[{
  RowBox[{"myMetropolisHastings", "[", 
   RowBox[{
   "function_", ",", "start_", ",", "variance_", ",", "iterations_", ",", 
    "burnin_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"myList", "=", 
       RowBox[{"{", "start", "}"}]}], ",", 
      RowBox[{"myDistribution", "=", 
       RowBox[{"MultinormalDistribution", "[", 
        RowBox[{
         RowBox[{"ConstantArray", "[", 
          RowBox[{"0", ",", 
           RowBox[{"Length", "[", "start", "]"}]}], "]"}], ",", 
         RowBox[{"DiagonalMatrix", "[", 
          RowBox[{"ConstantArray", "[", 
           RowBox[{"variance", ",", 
            RowBox[{"Length", "[", "start", "]"}]}], "]"}], "]"}]}], 
        "]"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"With", "[", 
