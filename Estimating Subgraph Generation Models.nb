Cell[BoxData[
 RowBox[{
  RowBox[{"levelThreeCountSubgraphs", "[", 
   RowBox[{"n_", ",", "edgeList_"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"adjacencyMatrix", "=", 
      RowBox[{"SparseArray", "[", 
       RowBox[{
        RowBox[{"AdjacencyMatrix", "[", "edgeList", "]"}], ",", 
