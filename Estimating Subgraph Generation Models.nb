Cell[BoxData[
 RowBox[{
  RowBox[{"myMetropolisHastings", "[", 
   RowBox[{
   "function_", ",", "start_", ",", "variance_", ",", "iterations_", ",", 
    "burnin_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"myList", "=", 
       RowBox[{"{", "start", "}"}]}], ",", 
      RowBox[{"myDistribution", "=", 
       RowBox[{"MultinormalDistribution", "[", 
        RowBox[{
         RowBox[{"ConstantArray", "[", 
          RowBox[{"0", ",", 
           RowBox[{"Length", "[", "start", "]"}]}], "]"}], ",", 
         RowBox[{"DiagonalMatrix", "[", 
          RowBox[{"ConstantArray", "[", 
           RowBox[{"variance", ",", 
            RowBox[{"Length", "[", "start", "]"}]}], "]"}], "]"}]}], 
        "]"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"With", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"myPrevious", "=", 
            RowBox[{"Last", "[", "myList", "]"}]}], ",", 
           RowBox[{"myProposal", "=", 
            RowBox[{
             RowBox[{"Last", "[", "myList", "]"}], "+", 
             RowBox[{"RandomVariate", "[", "myDistribution", "]"}]}]}], ",", 
           RowBox[{"myThreshold", "=", 
            RowBox[{"RandomReal", "[", "]"}]}]}], "}"}], ",", 
         RowBox[{"With", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"proposedValue", "=", 
             RowBox[{"function", "[", "myProposal", "]"}]}], "}"}], ",", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Element", "[", 
               RowBox[{"proposedValue", ",", "Reals"}], "]"}], "&&", 
              RowBox[{"proposedValue", ">", 
               RowBox[{"myThreshold", " ", 
                RowBox[{"function", "[", "myPrevious", "]"}]}]}]}], ",", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"myList", ",", "myProposal"}], "]"}], ",", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"myList", ",", "myPrevious"}], "]"}]}], "]"}]}], 
          "]"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "iterations"}], "}"}]}], "]"}], ";", 
     RowBox[{"myList", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"1", "+", "burnin"}], ";;", 
        RowBox[{"Length", "[", "myList", "]"}]}], "]"}], "]"}]}]}], 
   "]"}]}]], "Input",
