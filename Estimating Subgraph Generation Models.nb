         RowBox[{"{", 
          RowBox[{
           RowBox[{"myPrevious", "=", 
            RowBox[{"Last", "[", "myList", "]"}]}], ",", 
           RowBox[{"myProposal", "=", 
            RowBox[{
             RowBox[{"Last", "[", "myList", "]"}], "+", 
             RowBox[{"RandomVariate", "[", "myDistribution", "]"}]}]}], ",", 
           RowBox[{"myThreshold", "=", 
            RowBox[{"RandomReal", "[", "]"}]}]}], "}"}], ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"function", "[", "myProposal", "]"}], ">", 
            RowBox[{"myThreshold", " ", 
             RowBox[{"function", "[", "myPrevious", "]"}]}]}], ",", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"myList", ",", "myProposal"}], "]"}], ",", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"myList", ",", "myPrevious"}], "]"}]}], "]"}]}], "]"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "iterations"}], "}"}]}], "]"}], ";", 
     RowBox[{"myList", "[", 
      RowBox[{"[", 
       RowBox[{
        RowBox[{"1", "+", "burnin"}], ";;", 
        RowBox[{"Length", "[", "myList", "]"}]}], "]"}], "]"}]}]}], 
