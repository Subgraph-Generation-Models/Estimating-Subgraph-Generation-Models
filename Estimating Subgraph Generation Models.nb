         RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}], "}"}], ",", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"myPartitions", "=", 
         RowBox[{"levelThreePartitions", "[", "n", "]"}]}], ",", 
        RowBox[{"myCount", "=", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "0", ",", "0"}], "}"}]}]}], "}"}], ",", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"With", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"myTotal", "=", 
             RowBox[{
              RowBox[{"adjacencyMatrix", "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "1"}], "]"}], "]"}], ",", 
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "2"}], "]"}], "]"}]}], "]"}], "]"}], "+", 
              RowBox[{"adjacencyMatrix", "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "1"}], "]"}], "]"}], ",", 
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "3"}], "]"}], "]"}]}], "]"}], "]"}], "+", 
              RowBox[{"adjacencyMatrix", "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "2"}], "]"}], "]"}], ",", 
                 RowBox[{"myPartitions", "[", 
                  RowBox[{"[", 
                   RowBox[{"i", ",", "3"}], "]"}], "]"}]}], "]"}], "]"}]}]}], 
            "}"}], ",", 
           RowBox[{"If", "[", 
            RowBox[{
