                 RowBox[{
                  RowBox[{"myCount", "[", 
                   RowBox[{"[", "4", "]"}], "]"}], "++"}]}], "]"}]}], "]"}]}],
             "]"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           RowBox[{"Length", "[", "myPartitions", "]"}]}], "}"}]}], "]"}], 
       ";", "myCount"}]}], "]"}]}], "]"}]}]], "Input",
